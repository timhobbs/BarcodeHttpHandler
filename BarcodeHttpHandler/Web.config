<configuration>
    <system.web>
        <compilation debug="true" targetFramework="4.0" />
        <httpHandlers>
            <add verb="POST,GET" path="BarcodeHandler.axd" type="BarcodeHttpHandler.BarcodeHandler, BarcodeHttpHandler"/>
        </httpHandlers>
    </system.web>
    <system.webServer>
        <handlers>
            <add name="BarcodeHandler" verb="POST,GET" path="BarcodeHandler.axd" preCondition="integratedMode" type="BarcodeHttpHandler.BarcodeHandler, BarcodeHttpHandler"/>
        </handlers>
    </system.webServer>
</configuration>
